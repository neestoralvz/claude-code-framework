{
  "dashboard_configuration": {
    "name": "Claude Code System Health Dashboard",
    "version": "1.0.0",
    "refresh_rate": "1_second",
    "auto_refresh": true,
    "display_mode": "real_time_streaming"
  },
  "layout_structure": {
    "grid_system": "responsive_4x4",
    "panel_organization": "priority_based",
    "color_scheme": "status_indicator",
    "accessibility": "full_compliance"
  },
  "dashboard_panels": {
    "system_status_overview": {
      "position": {"row": 1, "col": 1, "span": "2x1"},
      "panel_type": "status_indicator_grid",
      "data_sources": [
        "operations/monitoring/metrics/realtime/system_status.json",
        "operations/monitoring/metrics/compliance/protocol_status.json"
      ],
      "display_elements": {
        "protocol_compliance_status": {
          "type": "status_light",
          "states": ["compliant", "warning", "violation"],
          "colors": ["green", "yellow", "red"],
          "threshold_mapping": "alert_thresholds.compliance_thresholds"
        },
        "command_delegation_health": {
          "type": "progress_indicator",
          "range": "0_to_100_percent",
          "target_value": "100_percent",
          "warning_threshold": "95_percent"
        },
        "session_state_validation": {
          "type": "status_badge",
          "states": ["healthy", "degraded", "critical"],
          "auto_update": "real_time"
        },
        "framework_integrity": {
          "type": "health_meter",
          "scale": "0_to_100",
          "zones": ["critical", "warning", "optimal"]
        }
      }
    },
    "performance_metrics_display": {
      "position": {"row": 1, "col": 3, "span": "2x2"},
      "panel_type": "real_time_charts",
      "data_sources": [
        "operations/monitoring/metrics/performance/execution_times.json",
        "operations/monitoring/metrics/performance/resource_usage.json"
      ],
      "chart_configurations": {
        "command_execution_time": {
          "chart_type": "line_graph",
          "time_range": "last_5_minutes",
          "y_axis": "seconds",
          "target_line": "1_second",
          "warning_zone": "0.8_to_1.0_seconds"
        },
        "system_resource_usage": {
          "chart_type": "area_chart",
          "time_range": "last_10_minutes", 
          "y_axis": "percentage",
          "target_ceiling": "10_percent",
          "warning_zone": "8_to_10_percent"
        },
        "monitoring_overhead": {
          "chart_type": "gauge",
          "current_value": "real_time",
          "max_value": "15_percent",
          "target_value": "under_10_percent"
        }
      }
    },
    "alert_management_interface": {
      "position": {"row": 2, "col": 1, "span": "2x1"},
      "panel_type": "alert_console",
      "data_sources": [
        "operations/monitoring/alerts/active_alerts.json",
        "operations/monitoring/alerts/alert_history.json"
      ],
      "interface_elements": {
        "active_alerts_list": {
          "display_format": "prioritized_list",
          "max_visible": "10_alerts",
          "auto_scroll": "enabled",
          "severity_color_coding": "standard"
        },
        "alert_statistics": {
          "total_alerts_today": "counter",
          "false_positive_rate": "percentage",
          "average_resolution_time": "duration"
        },
        "escalation_status": {
          "display_format": "status_timeline",
          "escalation_levels": ["level_1", "level_2", "level_3"],
          "current_level_highlight": "enabled"
        }
      }
    },
    "integration_status_monitoring": {
      "position": {"row": 3, "col": 1, "span": "4x1"},
      "panel_type": "integration_health_grid",
      "data_sources": [
        "operations/monitoring/metrics/integrations/matrix_status.json",
        "operations/monitoring/metrics/integrations/verifier_status.json"
      ],
      "status_indicators": {
        "command_reference_matrix": {
          "connection_status": "status_light",
          "data_flow_rate": "throughput_meter",
          "last_remediation": "timestamp",
          "success_rate": "percentage_indicator"
        },
        "protocol_compliance_verifier": {
          "sync_status": "status_light",
          "validation_rate": "rate_meter",
          "last_violation_check": "timestamp",
          "compliance_score": "score_indicator"
        },
        "cross_system_communication": {
          "latency": "latency_meter",
          "error_rate": "error_percentage",
          "data_consistency": "consistency_indicator",
          "throughput": "data_rate_meter"
        }
      }
    },
    "predictive_analysis_display": {
      "position": {"row": 4, "col": 1, "span": "4x1"},
      "panel_type": "prediction_dashboard",
      "data_sources": [
        "operations/monitoring/metrics/trends/prediction_data.json",
        "operations/monitoring/metrics/trends/pattern_analysis.json"
      ],
      "prediction_elements": {
        "performance_trend_forecast": {
          "chart_type": "trend_projection",
          "time_horizon": "next_4_hours",
          "confidence_bands": "enabled",
          "intervention_points": "highlighted"
        },
        "compliance_drift_prediction": {
          "chart_type": "drift_indicator",
          "measurement_window": "last_2_hours",
          "prediction_accuracy": "confidence_percentage",
          "recommended_actions": "action_list"
        },
        "system_health_trajectory": {
          "chart_type": "health_projection",
          "multiple_scenarios": "best_worst_expected",
          "decision_points": "marked",
          "optimization_opportunities": "highlighted"
        }
      }
    }
  },
  "interactive_features": {
    "drill_down_capabilities": {
      "enabled": true,
      "levels": ["summary", "detailed", "granular"],
      "navigation": "click_based"
    },
    "real_time_filtering": {
      "enabled": true,
      "filter_types": ["time_range", "severity_level", "component_type"],
      "persistent_settings": "user_session"
    },
    "export_functionality": {
      "enabled": true,
      "formats": ["json", "csv", "pdf_report"],
      "automated_reports": "scheduled_generation"
    },
    "alert_management": {
      "acknowledge_alerts": "one_click",
      "suppress_alerts": "configurable_duration",
      "escalate_manually": "emergency_override"
    }
  },
  "customization_options": {
    "panel_arrangement": "drag_and_drop",
    "threshold_adjustment": "real_time_editing",
    "color_scheme_selection": "multiple_themes",
    "data_retention_settings": "user_configurable"
  },
  "performance_optimization": {
    "data_sampling": "intelligent_sampling",
    "chart_optimization": "canvas_based_rendering",
    "memory_management": "automatic_cleanup",
    "bandwidth_optimization": "delta_updates_only"
  }
}
